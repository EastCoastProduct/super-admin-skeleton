{
  "name": "super-admin-skeleton",
  "version": "1.0.0",
  "description": "ECP skeleton for React/Redux super admin application",
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack.prod.config.js --progress --profile --colors",
    "snapshots": "jest -u",
    "test": "jest",
    "start": "node prod.js",
    "dev-start": "node server.js",
    "heroku-postbuild": "npm run build"
  },
  "author": "ECP",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EastCoastProduct/super-admin-skeleton"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "fetch-mock": "^5.5.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^17.0.3",
    "postcss-js": "^0.1.3",
    "postcss-loader": "^1.2.0",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "redux-mock-store": "^1.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "aphrodite": "^1.1.0",
    "babel-polyfill": "^6.20.0",
    "es6-promise": "^4.0.5",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "normalize.css": "^5.0.0",
    "query-string": "^4.2.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "^2.1.0",
    "store": "^1.3.20",
    "validator": "^6.2.0",
    "whatwg-fetch": "^2.0.1"
  },
  "engines": {
    "node": ">=6.7 <7"
  },
  "jest": {
    "cacheDirectory": "../test/cache",
    "collectCoverage": true,
    "collectCoverageFrom": ["**/*.js"],
    "coverageDirectory": "../test/coverage",
    "coveragePathIgnorePatterns": [
      "styles.js",
      "<rootDir>/index.js",
      "<rootDir>/constants",
      "<rootDir>/fixtures/",
      "<rootDir>/routes/",
      "<rootDir>/styles/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90
      }
    },
    "globals": {
      "__API_URL__": "http://localhost:3000"
    },
    "rootDir": "src",
    "testRegex": "^.*test.js$",
    "timers": "fake"
  }
}
